{
    "name": "peec/peec-extension",
    "type": "typo3-cms-extension",
    "description": "This extension is built for the website of the research group peec. \nIt contains the connections and the interplay between the components employees, companies, projects and publications.",
    "authors": [
        {
            "name": "Daniel Saiz",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "PEEC\\PeecExtension\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PEEC\\PeecExtension\\Tests\\": "Tests"
        }
    },
    "replace": {
        "peec/peec-extension": "self.version",
        "typo3-ter/peec-extension": "self.version"
    }
}